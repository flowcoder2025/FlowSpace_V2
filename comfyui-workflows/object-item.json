{
  "_meta": {
    "name": "Object Item Generator",
    "description": "DreamShaper 8로 오브젝트/아이템 이미지를 생성합니다",
    "version": "1.0.0",
    "assetType": "object",
    "outputFormat": {
      "width": 512,
      "height": 512
    },
    "parameters": {
      "prompt": { "nodeId": "6", "field": "text", "type": "string" },
      "seed": { "nodeId": "3", "field": "seed", "type": "number" },
      "width": { "nodeId": "5", "field": "width", "type": "number", "default": 512 },
      "height": { "nodeId": "5", "field": "height", "type": "number", "default": 512 }
    }
  },
  "3": {
    "class_type": "KSampler",
    "inputs": {
      "seed": 0,
      "steps": 30,
      "cfg": 7.5,
      "sampler_name": "euler_ancestral",
      "scheduler": "normal",
      "denoise": 1.0,
      "model": ["4", 0],
      "positive": ["6", 0],
      "negative": ["7", 0],
      "latent_image": ["5", 0]
    }
  },
  "4": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "DreamShaper_8_pruned.safetensors"
    }
  },
  "5": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": 512,
      "height": 512,
      "batch_size": 1
    }
  },
  "6": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "pixel art game object, item icon, transparent background, clean edges, game asset, top-down RPG style",
      "clip": ["4", 1]
    }
  },
  "7": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "blurry, low quality, watermark, text, realistic, photo, 3d render, complex background",
      "clip": ["4", 1]
    }
  },
  "8": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["3", 0],
      "vae": ["4", 2]
    }
  },
  "9": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "object_item",
      "images": ["8", 0]
    }
  }
}
