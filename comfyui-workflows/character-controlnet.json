{
  "_meta": {
    "name": "Character Sprite Generator (ControlNet)",
    "description": "ControlNet 포즈 가이드를 사용하여 4방향 캐릭터 스프라이트시트를 생성합니다",
    "version": "1.0.0",
    "assetType": "character",
    "outputFormat": {
      "width": 1024,
      "height": 512,
      "grid": "8x4",
      "frameSize": "128x128"
    },
    "parameters": {
      "prompt": { "nodeId": "6", "field": "text", "type": "string" },
      "negative_prompt": { "nodeId": "7", "field": "text", "type": "string" },
      "seed": { "nodeId": "3", "field": "seed", "type": "number" },
      "steps": { "nodeId": "3", "field": "steps", "type": "number", "default": 35 },
      "cfg": { "nodeId": "3", "field": "cfg", "type": "number", "default": 7 },
      "sampler_name": { "nodeId": "3", "field": "sampler_name", "type": "string", "default": "euler_ancestral" },
      "scheduler": { "nodeId": "3", "field": "scheduler", "type": "string", "default": "normal" },
      "width": { "nodeId": "5", "field": "width", "type": "number", "default": 1024 },
      "height": { "nodeId": "5", "field": "height", "type": "number", "default": 512 },
      "controlnet_model": { "nodeId": "10", "field": "control_net_name", "type": "string", "default": "control_v11p_sd15_openpose.pth" },
      "controlnet_strength": { "nodeId": "11", "field": "strength", "type": "number", "default": 0.8 },
      "pose_image": { "nodeId": "12", "field": "image", "type": "string", "default": "pose-4dir-spritesheet.png" }
    }
  },
  "3": {
    "class_type": "KSampler",
    "inputs": {
      "seed": 0,
      "steps": 35,
      "cfg": 7,
      "sampler_name": "euler_ancestral",
      "scheduler": "normal",
      "denoise": 1.0,
      "model": ["4", 0],
      "positive": ["11", 0],
      "negative": ["11", 1],
      "latent_image": ["5", 0]
    }
  },
  "4": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "pixelArtDiffusionXL_spriteShaper.safetensors"
    }
  },
  "5": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": 1024,
      "height": 512,
      "batch_size": 1
    }
  },
  "6": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "pixel art character sprite sheet, 8 frames walk cycle, 4 directions (down, left, right, up), 128x128 pixel per frame, 8x4 grid layout, transparent background, game asset, top-down RPG style",
      "clip": ["4", 1]
    }
  },
  "7": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "blurry, low quality, watermark, text, realistic, photo, 3d render, deformed, ugly",
      "clip": ["4", 1]
    }
  },
  "8": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["3", 0],
      "vae": ["4", 2]
    }
  },
  "9": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "character_controlnet",
      "images": ["8", 0]
    }
  },
  "10": {
    "class_type": "ControlNetLoader",
    "inputs": {
      "control_net_name": "control_v11p_sd15_openpose.pth"
    }
  },
  "11": {
    "class_type": "ControlNetApply",
    "inputs": {
      "conditioning": ["6", 0],
      "control_net": ["10", 0],
      "image": ["12", 0],
      "strength": 0.8
    }
  },
  "12": {
    "class_type": "LoadImage",
    "inputs": {
      "image": "pose-4dir-spritesheet.png",
      "upload": "image"
    }
  }
}
