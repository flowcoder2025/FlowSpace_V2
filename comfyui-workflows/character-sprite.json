{
  "_meta": {
    "name": "Character Sprite Generator",
    "description": "Pixel Art Diffusion XL로 4방향 캐릭터 스프라이트시트를 생성합니다",
    "version": "2.0.0",
    "assetType": "character",
    "outputFormat": {
      "width": 1024,
      "height": 512,
      "grid": "8x4",
      "frameSize": "128x128"
    },
    "parameters": {
      "prompt": { "nodeId": "6", "field": "text", "type": "string" },
      "negative_prompt": { "nodeId": "7", "field": "text", "type": "string" },
      "seed": { "nodeId": "3", "field": "seed", "type": "number" },
      "steps": { "nodeId": "3", "field": "steps", "type": "number", "default": 30 },
      "cfg": { "nodeId": "3", "field": "cfg", "type": "number", "default": 7.5 },
      "sampler_name": { "nodeId": "3", "field": "sampler_name", "type": "string", "default": "dpmpp_2m" },
      "scheduler": { "nodeId": "3", "field": "scheduler", "type": "string", "default": "karras" },
      "width": { "nodeId": "5", "field": "width", "type": "number", "default": 1024 },
      "height": { "nodeId": "5", "field": "height", "type": "number", "default": 512 }
    }
  },
  "3": {
    "class_type": "KSampler",
    "inputs": {
      "seed": 0,
      "steps": 30,
      "cfg": 7.5,
      "sampler_name": "dpmpp_2m",
      "scheduler": "karras",
      "denoise": 1.0,
      "model": ["4", 0],
      "positive": ["6", 0],
      "negative": ["7", 0],
      "latent_image": ["5", 0]
    }
  },
  "4": {
    "class_type": "CheckpointLoaderSimple",
    "inputs": {
      "ckpt_name": "pixelArtDiffusionXL_spriteShaper.safetensors"
    }
  },
  "5": {
    "class_type": "EmptyLatentImage",
    "inputs": {
      "width": 1024,
      "height": 512,
      "batch_size": 1
    }
  },
  "6": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "pixel art character sprite sheet, 8 frames walk cycle, 4 directions (down, left, right, up), 128x128 pixel per frame, 8x4 grid layout, transparent background, game asset, top-down RPG style, centered character in each frame, consistent size across all frames, uniform scale, aligned poses",
      "clip": ["4", 1]
    }
  },
  "7": {
    "class_type": "CLIPTextEncode",
    "inputs": {
      "text": "blurry, low quality, watermark, text, realistic, photo, 3d render, deformed, ugly, different sizes, varying scale, inconsistent proportions, misaligned, off-center, cropped, cut off, partial character, overlapping frames",
      "clip": ["4", 1]
    }
  },
  "8": {
    "class_type": "VAEDecode",
    "inputs": {
      "samples": ["3", 0],
      "vae": ["4", 2]
    }
  },
  "9": {
    "class_type": "SaveImage",
    "inputs": {
      "filename_prefix": "character_sprite",
      "images": ["8", 0]
    }
  }
}
